            arcpy.management.JoinField(os.path.join(entrada,'temp_DrenajeNamed_centeredline'), "FID", temp_DrenajeNamed, "OBJECTID", "NOMBRE_GEOGRAFICO", "NOT_USE_FM", None)                                      
